    "title": "Product Detail Cards Shipping Cont",
    "children": [
      "flex-layout.col#product__cards--shipping"
    ],
    "props": {
      "border": "all",
      "borderWidth": 2,
      "borderColor": "black",
      "blockClass": "product__cards--shipping-cont"
    }
